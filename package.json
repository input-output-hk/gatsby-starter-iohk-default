{
  "name": "iohk-gatsby-starter",
  "description": "IOHK Gatsby starter",
  "version": "0.0.1",
  "author": "Lewis Nelson <lewis.nelson@iohk.io>",
  "dependencies": {
    "@input-output-hk/front-end-core-components": "^1.0.1",
    "@input-output-hk/front-end-core-libraries": "^1.0.1",
    "@input-output-hk/front-end-site-components": "^1.0.0",
    "@input-output-hk/front-end-themes": "^1.0.0",
    "@material-ui/core": "^4.9.5",
    "@rhysforyou/gatsby-plugin-react-helmet-async": "^0.1.3",
    "gatsby": "^2.19.22",
    "gatsby-plugin-manifest": "^2.2.42",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-offline": "^3.0.35",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-remark": "2.6.53",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-ga": "^2.7.0",
    "styled-components": "^5.0.1",
    "whatwg-fetch": "^3.0.0"
  },
  "keywords": [
    "gatsby",
    "starter",
    "iohk"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && gatsby build && npm run build:netlify-prod",
    "build:netlify": "webpack --config webpack.netlify.config.js",
    "build:netlify-prod": "NODE_ENV=production webpack --config webpack.netlify.config.js",
    "clean": "gatsby clean",
    "create-pages": "node ./scripts/create-pages.js",
    "develop": "gatsby develop",
    "lint": "eslint src/",
    "lint:changed": "eslint",
    "postinstall": "./scripts/post-install.sh",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "serve:coverage": "open coverage/lcov-report/index.html",
    "watch:netlify": "NODE_ENV=development npm run build:netlify -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.2.29",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-jsx": "^8.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/input-output-hk/iohk-gatsby-starter"
  }
}
